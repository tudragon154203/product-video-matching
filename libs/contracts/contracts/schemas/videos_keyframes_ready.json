{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VideosKeyframesReady",
  "type": "object",
  "additionalProperties": true,
  "required": ["video_id", "frames"],
  "properties": {
    "video_id": {
      "type": "string",
      "description": "Unique identifier for the video"
    },
    "frames": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["frame_id", "ts", "local_path"],
        "properties": {
          "frame_id": {
            "type": "string",
            "description": "Unique identifier for the frame"
          },
          "ts": {
            "type": "number",
            "description": "Timestamp in video (seconds)"
          },
          "local_path": {
            "type": "string",
            "description": "Local file system path to the frame"
          }
        }
      }
    }
  }
}